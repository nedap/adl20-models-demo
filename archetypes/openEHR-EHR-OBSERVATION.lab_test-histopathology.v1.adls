										}
									}
								}
							}
							CLUSTER[id0.87] occurrences matches {0..1} matches {	-- Microscopic findings
								items matches {
									CLUSTER[id0.81] occurrences matches {0..*} matches {	-- Microscopic finding
										items matches {
