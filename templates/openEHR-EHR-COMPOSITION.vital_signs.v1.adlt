            ["id1007.1"] = <
                text = <"Mittlerer arterieller Druck">
                description = <"Der mittlerer arterielle Druck über den gesamten Verlauf der Konktraktions- und Entspannungsphase des Herzens.">
            >
            ["id5.1"] = <
                text = <"Systolisch">
