definition
    OBSERVATION[id1.1] matches {    -- Blood Pressure
        /data[id2]/events matches {
            EVENT[id7] occurrences matches {0..*} matches {
                /data[id4]/items matches {
                    ELEMENT[id5.1] occurrences matches {1} matches {    -- Systolic
                        value matches {
                            DV_QUANTITY[id1054.1] matches {
                                [units, magnitude, precision] matches {
                                    [{"mm[Hg]"}, {|0.0..<1000.0|}, {|>=0|}]
                                }
                            }
                        }
                    }
                    ELEMENT[id6.1] occurrences matches {1} matches {    -- Diastolic
                        value matches {
                            DV_QUANTITY[id1055.1] matches {
                                [units, magnitude, precision] matches {
                                    [{"mm[Hg]"}, {|0.0..<1000.0|}, {|>=0|}]
                                }
                            }
                        }
                    }
                    ELEMENT[id1007.1] occurrences matches {0} matches {    -- Mean Arterial Pressure
                        value matches {
                            DV_QUANTITY[id1056.1] matches {
                                property matches {[at1055]}
                                magnitude matches {|0.0..<1000.0|}
                                precision matches {|0|}
                                units matches {"mm[Hg]"}
                            }
                        }
                    }
                }
            }
            INTERVAL_EVENT[id1043.1] occurrences matches {0}     -- 24 hour average 
        }
        /protocol[id12]/items matches {
            ELEMENT[id1039.1] occurrences matches {0} matches {    -- Mean Arterial Pressure Formula
                value matches {
                    DV_TEXT[id1071.1] 
                }
            }
        }
    }

terminology
    term_definitions = <
        ["en"] = <
